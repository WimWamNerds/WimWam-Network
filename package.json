    "sequelize-cli": "6.2.0",
    "serve-static": "^1.14.1",
    "sharp": "^0.25.3",
    "sqlite3": "^5.0.2",
    "typedarray-to-buffer": "^3.1.5",
    "unzip-stream": "^0.3.0",
    "uuid-apikey": "^1.4.3"
