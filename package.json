{
  "name": "geesome-node",
  "version": "0.2.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "yarn && npm run aqua-compile-communicator && DEBUG=geesome* ./node_modules/.bin/ts-node ./index.ts",
    "in-docker-start": "cd /geesome-node/frontend && npm run build-new-ver && cp -R /geesome-node/frontend/dist /geesome-node/frontend/docker-dist && cd /geesome-node && npm run start",
    "recreate-database": "dropdb geesome_node && createdb geesome_node",
    "recreate-test-database": "dropdb geesome_test && createdb geesome_test",
    "migrate-database": "( cd components/database/sql && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path components/database/sql/migrations --name",
    "rollback-migration": "( cd components/database/sql && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate:undo)",
    "aqua-compile-communicator": "aqua-cli -i ./components/communicator/fluence/aqua -o ./components/communicator/fluence",
    "check-perfomance": "npm run recreate-test-database; rm -rf ./.jsipfs-test; ts-node check/geesomePerfomance.ts --trace_gc",
    "check-client-perfomance": "node --max-old-space-size=4096 -- node_modules/.bin/ts-node -P tsconfig.json check/geesomeClientPerfomance.ts",
    "generate-docs": "./node_modules/.bin/apidoc -i components/api/http-v1 -o docs -t node_modules/apidoc-template/template && ./node_modules/.bin/http-server ./docs",
    "test": "rm -rf database-test.sqlite; rm -rf ./.jsipfs-test; DEBUG=geesome* ./node_modules/.bin/mocha --exit -r ts-node/register 'test/**/*.test.ts'",
    "testCoverage": "./node_modules/.bin/nyc -r lcov -e .ts -x \"*.test.ts\" ./node_modules/.bin/mocha -r ts-node/register test/**/*.test.ts && nyc report",
    "docker": "STORAGE_STAGING=$PWD/.docker-data/ipfs-staging STORAGE_DATA=$PWD/.docker-data/ipfs GEESOME_DATA=$PWD/.docker-data/geesome-data GEESOME_FRONTEND_DIST=$PWD/.docker-data/geesome-frontend docker-compose up",
    "docker-build": "bash/docker-build.sh",
    "docker-upgrade": "bash/docker-copy-and-upgrade.sh"
  },
  "author": "",
  "license": "ISC",
